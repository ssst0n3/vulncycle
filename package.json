{
  "name": "vulncycleinsight",
  "version": "1.0.0",
  "description": "基于生命周期的漏洞研究报告编辑器",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js,css,html}\""
